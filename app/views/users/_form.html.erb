<%= bootstrap_form_for(@user) do |f| %>
  <%= render 'shared/error_messages', object: @user %>

  <%= f.field :text_field, :username %>

  <%= f.field :text_field, :email %>

  <%= f.field :password_field, :password %>

  <%= f.field :password_field, :password_confirmation, 'Confirm Password' %>

  <%= f.submit 'Sign up' %>

<% end %>
